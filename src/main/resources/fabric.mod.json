{
  "schemaVersion": 1,
  "id": "zen-additions",
  "version": "${version}",
  "name": "Zen Additions",
  "description": "An utility functionality focused addon for Meteor Client.",
  "authors": [
    "noEngineer/yesEngineer"
  ],
  "contact": {
    "repo": "https://github.com/yesEngineer/zen-additions"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "net.zen.addon.ZenHandler"
    ]
  },
  "mixins": [
    "zen-additions.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "252,177,244"
  },
  "depends": {
    "java": ">=21",
    "minecraft": "${mc_range}",
    "meteor-client": "*",
    "fabricloader": ">=0.16.0"
  }
}
